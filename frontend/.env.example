# Noorah Frontend Environment Variables

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3001/api/v1
EXPO_PUBLIC_WS_URL=ws://localhost:3001

# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789012
EXPO_PUBLIC_FIREBASE_APP_ID=1:123456789012:web:abcdef123456789012345

# Stripe Configuration (Public Keys Only)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Google Maps API Key
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_APP_VERSION=1.0.0

# Security Configuration
EXPO_PUBLIC_ENABLE_SECURITY_FEATURES=true
EXPO_PUBLIC_ENABLE_BIOMETRIC_AUTH=true
EXPO_PUBLIC_ENABLE_GPS_TRACKING=true

# Development Configuration
EXPO_PUBLIC_ENABLE_DEV_TOOLS=true
EXPO_PUBLIC_ENABLE_LOGGING=true
EXPO_PUBLIC_LOG_LEVEL=debug

# Sentry Configuration (Optional)
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn_here

# Analytics Configuration (Optional)
EXPO_PUBLIC_ANALYTICS_ENABLED=false
EXPO_PUBLIC_ANALYTICS_API_KEY=your_analytics_api_key_here

