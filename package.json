{
  "name": "nannyradar",
  "version": "1.0.0",
  "description": "Noorah - Quantum Babysitting Cloud",
  "main": "main.ts",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "build": "npm run backend:build && npm run frontend:build",
    "start": "node dist/main.js",
    "start:prod": "npm run build && npm run start",
    "backend:dev": "cd backend && npm run start:dev",
    "backend:build": "cd backend && npm run build",
    "frontend:dev": "cd frontend && npx expo start --web",
    "frontend:build": "cd frontend && npx expo build",
    "test": "npm run backend:test && npm run frontend:test",
    "backend:test": "cd backend && npm test",
    "frontend:test": "cd frontend && npm test",
    "clean": "rimraf backend/dist frontend/dist",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": [
    "babysitting",
    "childcare",
    "mobile-app",
    "nestjs",
    "react-native",
    "expo"
  ],
  "author": "Naimah Barnes",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "rimraf": "^5.0.10"
  },
  "workspaces": [
    "backend",
    "frontend"
  ]
}

