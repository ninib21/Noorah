{
  "name": "nannyradar-backend",
  "version": "1.0.0",
  "description": "Backend API for NannyRadar babysitting app",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:load": "k6 run test/load/load-test.js",
    "test:security": "npm run test:owasp && npm run test:snyk",
    "test:owasp": "zap-baseline.py -t http://localhost:3000",
    "test:snyk": "snyk test",
    "test:all": "npm run test && npm run test:e2e && npm run test:load && npm run test:security"
  },
  "keywords": ["babysitting", "api", "nestjs", "typescript"],
  "author": "GuardianNest Team",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/config": "^3.0.0",
    "typeorm": "^0.3.0",
    "pg": "^8.11.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "bcryptjs": "^2.4.3",
    "stripe": "^13.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.5.0",
    "@types/supertest": "^2.0.12",
    "@nestjs/testing": "^10.0.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.0",
    "k6": "^0.45.0",
    "snyk": "^1.1200.0"
  }
} 